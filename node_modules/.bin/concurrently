#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/F:/code/scoreLineHTML/node_modules/.store/concurrently@9.1.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/F:/code/scoreLineHTML/node_modules/.store/concurrently@9.1.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/concurrently@9.1.2/node_modules/concurrently/dist/bin/concurrently.js" "$@"
else
  exec node  "$basedir/../.store/concurrently@9.1.2/node_modules/concurrently/dist/bin/concurrently.js" "$@"
fi
